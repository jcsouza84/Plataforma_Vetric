{
  "name": "vetric-dashboard-backend",
  "version": "1.0.0",
  "description": "Backend VETRIC Dashboard - API com integração CVE-Pro e Evolution API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "db:init": "ts-node -e \"require('./src/config/database').initDatabase()\"",
    "migrate": "ts-node src/database/index.ts run",
    "migrate:status": "ts-node src/database/index.ts status",
    "migrate:rollback": "ts-node src/database/index.ts rollback",
    "seed:moradores": "ts-node run-seed-moradores.ts"
  },
  "keywords": [
    "vetric",
    "cve-pro",
    "charger",
    "dashboard"
  ],
  "author": "VETRIC",
  "license": "MIT",
  "dependencies": {
    "@stomp/stompjs": "^7.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.11.5",
    "@types/pg": "^8.16.0",
    "@types/ws": "^8.5.10",
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.3.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "socket.io": "^4.6.1",
    "typescript": "^5.3.3",
    "winston": "^3.11.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  }
}
